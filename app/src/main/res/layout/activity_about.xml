<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    tools:context=".AboutActivity"
    >

<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/about_all"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ScrollViewSize">

    <LinearLayout
        android:id="@+id/guide_boxes"
        android:orientation="vertical"
        android:weightSum="2"
        android:background="@drawable/border__all_drawable"
        android:visibility="gone"
        android:layout_marginTop="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/app_version_box"
            android:layout_weight="1"
            android:visibility="gone"
            android:layout_marginRight="4dp"
            android:layout_marginEnd="4dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/content_background"
            app:layout_constraintTop_toTopOf="parent"
            >
            <TextView
                android:id="@+id/app_version"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="App Version"
                android:textAllCaps="false"
                android:textColor="@color/colorText1"
                android:textSize="22sp"
                android:textStyle="bold"
                android:textAlignment="center"
                app:fontFamily="@font/averia_gruesa_libre"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                />
            <TextView
                android:id="@+id/app_version_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorText2"
                android:textSize="17sp"
                android:textAllCaps="false"
                android:text="1.0.1"
                android:fontFamily="@font/comfortaa_light"
                app:layout_constraintTop_toBottomOf="@id/app_version"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/check_for_update_box"
            android:layout_weight="1"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/content_background"
            app:layout_constraintTop_toBottomOf="@+id/app_version_box"
            >
            <TextView
                android:id="@+id/check_for_update_textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:textSize="22sp"
                android:textStyle="bold"
                android:textColor="@color/colorText1"
                android:text="Check for Updates"
                android:textAlignment="center"
                android:fontFamily="@font/averia_gruesa_libre"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                />
            <ImageButton
                android:id="@+id/check_for_update_btn"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginBottom="4dp"
                android:layout_marginTop="2dp"
                android:src="@drawable/ic_cloud_upload_black_24dp"
                android:background="@drawable/check_for_updates_btn_bg"
                app:layout_constraintTop_toBottomOf="@+id/check_for_update_textView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                tools:ignore="ContentDescription" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/developer_boxes"
        android:orientation="vertical"
        android:weightSum="2"
        android:background="@drawable/border__all_drawable"
        android:paddingBottom="6dp"
        android:visibility="visible"
        android:layout_marginTop="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/bottomExtraSpace"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        >

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/developers_name_box"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        >
        <TextView
            android:id="@+id/developers_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/colorText1"
            android:textSize="52sp"
            android:textAllCaps="false"
            android:textAlignment="center"
            android:text="Developers"
            android:fontFamily="@font/carter_one"
            android:layout_marginTop="-10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />
    </androidx.coordinatorlayout.widget.CoordinatorLayout>

    <!--Aniket-->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/about1_aniket"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:background="@drawable/content_background"
        app:layout_constraintTop_toBottomOf="@id/developers_name_box"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        >
        <ImageView
            android:id="@+id/about1_aniket_icon_imageView"
            android:layout_width="71dp"
            android:layout_height="71dp"
            android:src="@drawable/about1_aniket_pic"
            android:scaleType="fitCenter"
            android:transitionName="about1aniketImageView"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            tools:ignore="ContentDescription,UnusedAttribute" />
        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:id="@+id/about1_aniket_icon_progressBar_CoordinatorLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:transitionName="about1aniketProgressBarCoordinatorLayout"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            tools:ignore="UnusedAttribute">
            <ProgressBar
                android:id="@+id/about1_aniket_icon_progressBar1"
                android:indeterminateDrawable="@drawable/about_pic_line_1"
                android:layout_width="104dp"
                android:layout_height="104dp"
                android:max="100"
                android:progress="0"
                android:indeterminateDuration="24000"
                android:transitionName="about1aniketProgressBar1"
                android:layout_marginTop="-7dp"
                android:layout_marginLeft="-7dp"
                android:layout_marginStart="-7dp"
                />
            <ProgressBar
                android:id="@+id/about1_aniket_icon_progressBar2"
                android:indeterminateDrawable="@drawable/about_pic_line_2"
                android:layout_width="104dp"
                android:layout_height="104dp"
                android:max="100"
                android:progress="0"
                android:indeterminateDuration="24000"
                android:transitionName="about1aniketProgressBar2"
                android:layout_marginTop="-7.4dp"
                android:layout_marginLeft="-7dp"
                android:layout_marginStart="-7dp"
                />
        </androidx.coordinatorlayout.widget.CoordinatorLayout>
        <TextView
            android:id="@+id/about1_aniket_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="12dp"
            android:text="Aniket"
            android:textColor="@color/colorText1"
            android:textSize="25sp"
            app:fontFamily="@font/capriola"
            app:layout_constraintLeft_toRightOf="@+id/about1_aniket_icon_imageView"
            app:layout_constraintStart_toEndOf="@+id/about1_aniket_icon_imageView"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/about1_aniket_name_skills_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="3dp"
            android:layout_marginRight="3dp"
            android:text="Layout Designer"
            android:textColor="@color/colorText2"
            android:textSize="17sp"
            app:fontFamily="@font/cormorant_upright_semibold"
            app:layout_constraintBottom_toTopOf="@+id/about1_aniket_name_skills_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintRight_toRightOf="parent" />
        <TextView
            android:id="@+id/about1_aniket_name_skills_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorText2"
            android:textSize="17sp"
            android:text="Conceptual Programmer"
            android:layout_marginBottom="4dp"
            android:layout_marginRight="3dp"
            android:layout_marginEnd="3dp"
            app:fontFamily="@font/cormorant_upright_semibold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

