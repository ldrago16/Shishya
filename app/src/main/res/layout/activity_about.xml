            android:textSize="25sp"
            app:fontFamily="@font/capriola"
            app:layout_constraintLeft_toRightOf="@+id/about2_ayush_icon"
            app:layout_constraintStart_toEndOf="@+id/about2_ayush_icon"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/about2_ayush_name_skills_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="3dp"
            android:layout_marginRight="3dp"
            android:text="Database Handler"
            android:textColor="@color/colorText2"
            android:textSize="17sp"
            app:fontFamily="@font/cormorant_upright_semibold"
            app:layout_constraintBottom_toTopOf="@+id/about2_ayush_name_skills_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintRight_toRightOf="parent" />
